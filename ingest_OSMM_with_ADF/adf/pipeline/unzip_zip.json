{
	"name": "unzip_zip",
	"properties": {
		"activities": [
			{
				"name": "Unzip_to_bronze",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings"
							}
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "landed_zip",
						"type": "DatasetReference",
						"parameters": {
							"provider": {
								"value": "@pipeline().parameters.provider",
								"type": "Expression"
							},
							"dataset": {
								"value": "@pipeline().parameters.dataset",
								"type": "Expression"
							},
							"version": {
								"value": "@pipeline().parameters.version",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "bronze_unzipped",
						"type": "DatasetReference",
						"parameters": {
							"filetype": {
								"value": "@pipeline().parameters.filetype",
								"type": "Expression"
							},
							"provider": {
								"value": "@pipeline().parameters.provider",
								"type": "Expression"
							},
							"dataset": {
								"value": "@pipeline().parameters.dataset",
								"type": "Expression"
							},
							"version": {
								"value": "@pipeline().parameters.version",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"provider": {
				"type": "string"
			},
			"dataset": {
				"type": "string"
			},
			"version": {
				"type": "string"
			},
			"filetype": {
				"type": "string"
			}
		},
		"folder": {
			"name": "unzip"
		},
		"annotations": []
	}
}