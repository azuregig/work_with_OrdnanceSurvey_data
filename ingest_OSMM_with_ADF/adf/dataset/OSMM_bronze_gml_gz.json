{
	"name": "OSMM_bronze_gml_gz",
	"properties": {
		"linkedServiceName": {
			"referenceName": "datalakeexplore",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"package_id": {
				"type": "string"
			},
			"package_version": {
				"type": "string"
			},
			"filename": {
				"type": "string"
			}
		},
		"folder": {
			"name": "geodata/OSMM"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('gz/OSMM/v1/dataPackages/',dataset().package_id,'/version/',dataset().package_version)",
					"type": "Expression"
				},
				"fileSystem": "bronze"
			},
			"compression": {
				"type": "gzip"
			}
		}
	}
}