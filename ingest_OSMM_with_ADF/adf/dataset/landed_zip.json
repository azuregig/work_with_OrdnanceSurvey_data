{
	"name": "landed_zip",
	"properties": {
		"linkedServiceName": {
			"referenceName": "azuregiglanding",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"provider": {
				"type": "string"
			},
			"dataset": {
				"type": "string"
			},
			"version": {
				"type": "string"
			}
		},
		"folder": {
			"name": "generic/landing"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"folderPath": {
					"value": "@concat(dataset().provider,'/',dataset().dataset,'/',dataset().version)",
					"type": "Expression"
				},
				"container": "landing"
			}
		}
	}
}